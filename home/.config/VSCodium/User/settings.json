{
    "workbench.colorTheme": "Gruvbox Light (Medium)",
    "vim.easymotion": true,
    "vim.normalModeKeyBindings": [
        {
            "before": ["<Space>"],
            "after": [":"]
        },
        {
            "before":["f"],
            "after":["leader", "leader", "s"]
        },
        {
            "before":["g","r"],
            "after":["g", "T"]
        }
    ],
    "vim.insertModeKeyBindings": [
        {
          "before": ["j", "j"],
          "after": ["<Esc>"]
        }
      ],
    "vim.easymotionKeys": "asdfghjkl",
    "window.zoomLevel": 0,
    "editor.lineNumbers": "on"
}