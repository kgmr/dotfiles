!font: xft:Fira Mono:pixelsize=14,style=regular
!urxvt.font: xft:DejaVuSansMono:pixelsize=14
urxvt.font: xft:monaco:style=Regular:antialias=true:size=13
!font: xft:Dina:regular:pixelsize=13
!font: xft:Monaco:regular:antialias=true:pixelsize=12
!Scrollbar
Urxvt.scrollStyle:rxvt
scrollBar: false
urxvt*perl-lib:            /usr/lib/urxvt/perl/
urxvt*perl-ext-common:        default,matcher
urxvt*urlLauncher:        /usr/bin/firefox
urxvt*matcher.button:        1
urxvt*matcher.pattern.1:   \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]

rofi.padding : 20
rofi.font: Roboto Light 20
!rofi.fullscreen : true
rofi.display-drun:
rofi.display-window: win
rofi.display-combi: myprompt
!depth: 32
Xcursor.theme: Breeze_Light


!dwm.normfgcolor: #424242
!dwm.normbgcolor: #ffffec
!dwm.normbordercolor: #ffffec
!dwm.selfgcolor: #ffffec
!dwm.selbgcolor: #424242
!dwm.selbordercolor: #424242
