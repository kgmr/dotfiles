!URxvt.font: xft:Fira Mono:pixelsize=14,style=regular
!urxvt.font: xft:Meslo LG S DZ for Powerline:bold:pixelsize=13
URxvt*font: xft:Meslo LG M:Regular:pixelsize=14
URxvt*boldFont: xft:Meslo LG M:Bold:pixelsize=14
URxvt*italicFont: xft:Meslo LG M:Italic:pixelsize=14
URxvt*bolditalicFont: xft:Meslo LG M:Bold:Italic:pixelsize=14
!URxvt*font: xft:dina :Regular:pixelsize=13

!Scrollbar
Urxvt.scrollStyle:rxvt
URxvt.scrollBar: false

!Rofi
rofi.font: dina\ 8
rofi.color-enabled: true
rofi.color-window: #2d2d2d, #2d2d2d, #268bd2
rofi.color-normal: #2d2d2d, #ffffff, #2d2d2d, #268bd2, #ffffff
rofi.color-active: #2d2d2d, #268bd2, #2d2d2d, #268bd2, #205171
rofi.color-urgent: #2d2d2d, #f3843d, #2d2d2d, #268bd2, #ffc39c
rofi.width: 1152
rofi.lines: 8
rofi.location: 2
rofi.yoffset: 30
rofi.xoffset:-2
rofi.opacity: 98
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/chromium
URxvt.matcher.button: 1

Xcursor.theme: Human
