!font: xft:Fira Mono:pixelsize=14,style=regular
!urxvt.font: xft:DejaVuSansMono:pixelsize=14
urxvt.font: xft:monaco:style=Regular:antialias=true:size=13
!font: xft:Dina:regular:pixelsize=13
!font: xft:Monaco:regular:antialias=true:pixelsize=12
!Scrollbar
Urxvt.scrollStyle:rxvt
scrollBar: false
urxvt*perl-lib:            /usr/lib/urxvt/perl/
urxvt*perl-ext-common:        default,matcher
urxvt*urlLauncher:        /usr/bin/firefox
urxvt*matcher.button:        1
urxvt*matcher.pattern.1:   \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]

rofi.padding : 20
rofi.font: Roboto Light 20
!rofi.fullscreen : true
rofi.display-drun:
rofi.display-window: win
rofi.display-combi: myprompt
rofi.kb-row-up:                      Up,Control+k,Shift+Tab,Shift+ISO_Left_Tab
rofi.kb-row-down:                    Down,Control+j
rofi.kb-accept-entry:                Control+m,Return,KP_Enter
rofi.terminal:                       mate-terminal
rofi.kb-remove-to-eol:               Control+Shift+e
rofi.kb-mode-next:                   Shift+Right,Control+Tab,Control+l
rofi.kb-mode-previous:               Shift+Left,Control+Shift+Tab,Control+h
rofi.kb-remove-char-back:            BackSpace

!depth: 32
Xcursor.theme: Breeze_Light


!dwm.normfgcolor: #424242
!dwm.normbgcolor: #ffffec
!dwm.normbordercolor: #ffffec
!dwm.selfgcolor: #ffffec
!dwm.selbgcolor: #424242
!dwm.selbordercolor: #424242
! special
*.foreground:   #424242
*.background:   #ffffec
*.cursorColor:  #424242

! black
*.color0:       #424242
*.color8:       #eeeea7

! red
*.color1:       #b8261e
*.color9:       #f2acaa

! green
*.color2:       #3e8630
*.color10:      #98ce8f

! yellow
*.color3:       #7f8f29
*.color11:      #b6b79c

! blue
*.color4:       #2a8dc5
*.color12:      #a6dcf8

! magenta
*.color5:       #8888c7
*.color13:      #d0d1f7

! cyan
*.color6:       #6aa7a8
*.color14:      #b0eced

! white
*.color7:       #999957

*.color15:      #ffffec

Xft.autohint: 1
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

rofi.fg:                             #222222
rofi.bg:                             #fffbf8
rofi.fg-active:                      #5d729f
